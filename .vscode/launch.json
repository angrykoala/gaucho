{
  "version": "0.2.0",
  "configurations": [
      {
          "name": "Launch",
          "type": "node",
          "request": "launch",
          "program": "${workspaceRoot}/main.js",
          "stopOnEntry": false,
          "args": [],
          "cwd": "${workspaceRoot}",
          "preLaunchTask": null,
          "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/electron",
          "runtimeArgs": [
              ".",
              "--enable-logging"
          ],
          "env": {},
          "console": "internalConsole",
          "sourceMaps": false,
          "outDir": null
      },
      {
          "name": "Attach",
          "type": "node",
          "request": "attach",
          "port": 5858,
          "address": "localhost",
          "restart": false,
          "sourceMaps": false,
          "outDir": null,
          "localRoot": "${workspaceRoot}",
          "remoteRoot": null
      }
  ]
}